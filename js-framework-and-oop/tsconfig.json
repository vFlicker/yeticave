{
  "compilerOptions": {
      "target": "ES2017",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "Node",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./build"
    },
    "include": ["./src"],
    "exclude": ["node_modules"]
}